{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": [
        "string",
        "comment"
      ]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": [
        "string",
        "comment"
      ]
    },
    {
      "open": "\"\"\"",
      "close": "\"\"\"",
      "notIn": [
        "string",
        "comment"
      ]
    },
    {
      "open": "'''",
      "close": "'''",
      "notIn": [
        "string",
        "comment"
      ]
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ],
    [
      "'",
      "'"
    ]
  ],
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([A-Za-z_][\\w]*)",
  "indentationRules": {
    "increaseIndentPattern": "(\\{[^}\"']*$|\\([^)\"']*$|\\[[^\\]\"']*$)",
    "decreaseIndentPattern": "^\\s*[}\\])]"
  }
}
