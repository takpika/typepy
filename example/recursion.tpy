# 再帰関数のサンプル

def factorial(n: uint32) -> uint32 {
    if n == 0 {
        return 1;
    }
    return n * factorial(n - 1);
}

def fibonacci(n: uint32) -> uint32 {
    if n <= 1 {
        return n;
    }
    let a = fibonacci(n - 1);
    let b = fibonacci(n - 2);
    return a + b;
}

print(factorial(5));
print(fibonacci(8));
